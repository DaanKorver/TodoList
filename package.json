{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/*.html",
    "build": "parcel build src/*.html --public-url ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "parcel-bundler": "^1.12.5"
  },
  "devDependencies": {
    "parcel-plugin-custom-dist-structure": "^1.1.17",
    "sass": "^1.32.8",
    "typescript": "^4.2.3"
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      ".js.map": "js",
      ".css": "css",
      ".css.map": "css",
      "assets": [
        ".jpg",
        ".png"
      ],
      ".fileExtension": "folder/in/dist",
      "folder/in/dist": [
        ".file",
        ".extensions"
      ]
    },
    "options": {
      "development": true
    }
  }
}
